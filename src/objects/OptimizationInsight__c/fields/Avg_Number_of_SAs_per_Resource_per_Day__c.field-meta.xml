<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Avg_Number_of_SAs_per_Resource_per_Day__c</fullName>
    <externalId>false</externalId>
    <formula>IF(
(Number_Of_Internal_Resources__c+ Number_Of_Service_Crews__c)= 0, 0,
(Tasks_with_Internal_Resources_After__c / (Number_Of_Internal_Resources__c+ Number_Of_Service_Crews__c)/ (Optimization_Request__r.FSL__Finish__c - Optimization_Request__r.FSL__Start__c))
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Avg SAs per Resource per Day After</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
