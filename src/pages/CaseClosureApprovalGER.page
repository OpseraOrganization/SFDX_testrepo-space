    <script>
    
    $(document).ready(function(){
            var toHide = $("#noOfTimeContacted");
            toHide.hide();
        $("#confirmDiv").hide();
        
            $("input[name='satisfy']").change(function(){
                var satisfyVal = $(this).val();
                $("[id*='satisfyHidden']").val(satisfyVal);
            });
            
            $("input[name='extent']").change(function(){
                var extentVal = $(this).val();
                $("input[id*='extentHidden']").val(extentVal);
            });
            
            $("input[name='time']").change(function(){
                var timeVal = $(this).val();
                $("input[id*='timesHidden']").val(timeVal);
            });
            
            /* $("input[name='recommend']").change(function(){
                var recommendVal = $(this).val();
                $("input[id*='recommendHidden']").val(recommendVal);
            }); */
            
            $("#yes_1").click(function(){
                $("input[id*='conNumHidden']").val("1");
                $("#yes_1").css("background", "#1792E5");
                $("#no_2").css("background", "");
                var toHide = $("#noOfTimeContacted");
                toHide.hide();
            });
                
            $("#no_2").click(function(){
            $("input[id*='conNumHidden']").val("2");
                $("#no_2").css("background", "#1792E5");
                $("#yes_1").css("background", "");
                $("#noOfTimeContacted").show();
            });
        $("input[name='reasonforopen1']").change(function(){
                var reasonVal = $(this).val();
                $("input[id*='reasonforopenAOG']").val(reasonVal);
            });
            
            $("input[name='reasonforopen2']").change(function(){
                var reasonVal = $(this).val();
                $("input[id*='reasonforopenATS']").val(reasonVal);
            });
            
            $("input[name='phonecall1']").change(function(){
                var phoneVal = $(this).val();
                $("input[id*='phoneCallRequestAOG']").val(phoneVal);
            });
        
            $("input[name='phonecall2']").change(function(){
                var phoneVal = $(this).val();
                $("input[id*='phoneCallRequestATS']").val(phoneVal);
            });
        });
        
    </script>
    <script>
        function callOnsubmit()
        {
            var satisfyHidden2 = $("[id*='satisfyHidden']").val();
            var extentHidden2 = $("input[id*='extentHidden']").val();
            var timesHidden2 = $("input[id*='timesHidden']").val();
            //var recommendHidden2 = $("input[id*='recommendHidden']").val();
            var conNumHidden2 = $("input[id*='conNumHidden']").val();

            console.log('Called and ');
            console.log(satisfyHidden2);
            console.log(extentHidden2);
            console.log(timesHidden2);
            //console.log(recommendHidden2);
            console.log(conNumHidden2);
            initiatemethod(satisfyHidden2, extentHidden2, timesHidden2, conNumHidden2);
            cancelButton();
            return false;
        }
        function callOnReject()
        {
            var reasonforopenAOG = $("[id*='reasonforopenAOG']").val();
            var reasonforopenATS = $("input[id*='reasonforopenATS']").val();
            var phoneCallRequestAOG = $("input[id*='phoneCallRequestAOG']").val();
            var phoneCallRequestATS = $("input[id*='phoneCallRequestATS']").val();
            
            console.log('Called reject and ');
            console.log(reasonforopenAOG);
            console.log(reasonforopenATS);
            console.log(phoneCallRequestAOG);
            console.log(phoneCallRequestATS);
            initateReject(reasonforopenAOG, reasonforopenATS, phoneCallRequestAOG, phoneCallRequestATS);
            cancelButton();
            return false;
        }

        function cancelButton()
        {
            $("#maindiv").hide();
            $("#confirmDiv").show();
            return false;
        }
    </script>
