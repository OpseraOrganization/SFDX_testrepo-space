<script>
    var geocoder=null;
    var USFlag = false;
    var userAgent = navigator.userAgent;
    function resizeWindHome() {     
        if (userAgent.match(/(iPhone|iPod|Android|BlackBerry|IEMobile|Windows Phone)/g)) {    
            $("#home-page .aog-content").height($("html").height() - $(".ui-header").height()- $(".ui-footer").height() - 10);
        }
        else if (userAgent.match(/(iPad)/g)) { 
            $("#home-page .aog-content").height($("html").height() - $(".ui-header").height()- $(".ui-footer").height() - 80);
        }
        else {   
            $("#home-page .aog-content").height($("html").height()- $(".ui-header").height()- $(".ui-footer").height() - 20);
        }
    }
    $( window ).on( "orientationchange", function( event ) {
      //  resizeWindHome();
    }); 
    $(document).bind("pageshow", function (event, data) {
        if ((navigator.userAgent.toLowerCase().match(/android/) || navigator.userAgent.toLowerCase().match(/iphone/) || navigator.userAgent.toLowerCase().match(/ipad/)) && (typeof $.cookie("ss_created") === 'undefined') && getUrlParamByName('context') == 'container'){
            $.mobile.loading( "show" );
            //$.blockUI({ message: '<h3> Initializing application data...</h3>' }); 
            document.addEventListener("deviceready", initOfflineStorage, false);   
        }  
    });
    function initOfflineStorage() {
        navigator.splashscreen.hide();
        if ((navigator.userAgent.toLowerCase().match(/android/) || navigator.userAgent.toLowerCase().match(/iphone/) || navigator.userAgent.toLowerCase().match(/ipad/))  && (typeof $.cookie("ss_created") === 'undefined') && getUrlParamByName('context') == 'container'){  
              checkCacheAndPrepareSession();
        }
    }
    $(document).ready(function() {
        //Add to home screen plugin
        addToHomescreen({
           maxDisplayCount: 1
        });    
        //resizeWindHome();
        
        if (typeof $.cookie("the_cntyval") === 'undefined') {
            if (navigator.geolocation) {
                navigator.geolocation.getCurrentPosition(showPosition,showError);
            } 
        }  
        
        if (("standalone" in window.navigator) && window.navigator.standalone) {
            // For iOS Apps
            $('a').on('click', function(e){
              e.preventDefault();
              var new_location = $(this).attr('href');
              if (new_location != undefined && new_location.substr(0, 1) != '#' && $(this).attr('data-method') == undefined){
                window.location = new_location;
              }
            });
        }   
        $(window).resize(function(){
        //resizeWindHome();
        });   
    });
    function showPosition(position)
    {  
        geocoder = new google.maps.Geocoder();
        var lat=parseFloat(position.coords.latitude);
        var lng=parseFloat(position.coords.longitude);
        var latlng = new google.maps.LatLng(lat,lng);
        geocoder.geocode({'latLng': latlng}, function(results, status) {
            if (status == google.maps.GeocoderStatus.OK) {
                if (results[0]) {
                    var arrAddr = results[0].address_components;
                    var country;
                    $.each(arrAddr, function(i, address_component) {
                        if(address_component.types[0] == "country") {
                            country = address_component.long_name.toString();
                       
                            $.cookie('the_cntyval', country, { expires: 1, path: '/' });

                        }
                        if(address_component.types[0] == "administrative_area_level_1") {
                            state= address_component.long_name.toString();
                        //alert(state);
                            $.cookie('the_stateval', state, { expires: 1, path: '/' });
                        }
                    });
                }
            }         
        }); 
    }
    function showError(error)
    {
        switch(error.code) 
        {
        case error.PERMISSION_DENIED:
        //alert("User denied the request for Geolocation.");
          break;
        case error.POSITION_UNAVAILABLE:
        //alert("Location information is unavailable.");
          break;
        case error.TIMEOUT:
        //alert("The request to get user location timed out.");
          break;
        case error.UNKNOWN_ERROR:
        //alert("An unknown error occurred.");
          break;
        }
    }
</script>
