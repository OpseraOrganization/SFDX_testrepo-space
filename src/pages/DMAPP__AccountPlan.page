    <script>
    
        /* 
         * Copyright Â© {! JSENCODE($Setup.DMAPP__Company_Information__c.DMAPP__Company_Name__c)}. All rights reserved.
         *  
         */

        $.fn.addBack = function (selector) {
            return this.add(selector == null ? 
            this.prevObject : this.prevObject.filter(selector));
        }
        
        var spec = {};

        var filter = (function(_spec) {
            var that = {}

            function setup() {
                var range = "AZ";
                var i;
                for (i = range.charCodeAt(0); i <= range.charCodeAt(1); i++) {
                    
                    jQuery('.rolodex').append(
                        jQuery('<A>').addClass('listItem').append(
                            jQuery('<span>').addClass('listItemPad').text(String.fromCharCode(i)).click(filter) 
                        )
                    )
                
                }
                
                jQuery('.rolodex').append(
                    jQuery('<A>').addClass('listItem').append(
                        jQuery('<span>').addClass('listItemPad').text('{!JSENCODE($Label.COMMON_OTHER)}').click(filter)
                    )
                )
                .append(
                    jQuery('<A>').addClass('listItem').append(
                        jQuery('<span>').addClass('listItemPad').text('{!JSENCODE($Label.COMMON_ALL)}').click(filter)
                    )
                )
                
                jQuery('.ownership').change(ownershipChange);
                jQuery('.status').change(statusChange);
                jQuery('.less').click(less);
                jQuery('.more').click(more);
                                
                if ({!moreRecords == true && fewerRecords == true}) {
                    jQuery('#morefewerbar').removeClass('hidden')
                }
                else if ({!moreRecords == true}) { 
                    jQuery('#morebar').removeClass('hidden')
                }
                else if ({!fewerRecords == true}) {
                    jQuery('#fewerbar').removeClass('hidden')
                }
                
                if ('{!JSENCODE(sortOrder)}' == 'asc') {
                    jQuery('.sortDesc').css('display', 'none');             
                }
                else {
                    jQuery('.sortAsc').css('display', 'none');              
                }

                jQuery('.tab{!JSENCODE(sortField)}').removeClass('hidden')                
                                
            }
            
            function filter() {
                
                var label = jQuery(this).text();
                var value = label;
                if('{!JSENCODE($Label.COMMON_OTHER)}' == label) {
                    value = 'Other'
                } else if('{!JSENCODE($Label.COMMON_ALL)}' == label) {
                    value = 'All'
                }

                jQuery('.startsWith').val(value);
            
                filterPage();
                
            }

            function ownershipChange() {
                
                filterPage();
                
            }

            function statusChange() {
                
                filterPage();
                
            }

            function more() {
                
                filterMore();
                
            }

            function less() {
                
                filterLess();
                
            }
            
            function reload() {
                
                reloadPage();
                
            }

            that.setup = setup;
            that.reload = reload;
        
            return that;
        })(spec);
    
        jQuery(
        
            function () {
                filter.setup();
            }

        );
        
        help.showHelp = function() {
            window.open('{!JSENCODE(helpURLBase)}' + '/Default.htm#AM/AM.htm', "TAS", "resizable=1,location=1,status=1,scrollbars=1,width=1000,height=600");
        }

        var ttg = ttg || {};
        ttg.am = ttg.am || {};
    
        // This is referenced by OpportunityMapConfiguration, which is a staticresource, so need to ensure this is visible to it
        ttg.am.showSettingsHelp = function() {
            window.open('{!JSENCODE(helpURLBase)}' + "/Default.htm#AM/Plan_Creation/Plan_Creation.htm", "TAS", "resizable=1,location=1,status=1,scrollbars=1,width=1000,height=600");
        }

        ttg.linkClicked = function(url) {
            
            addOverlay();
        
            if(ttg_salesforce1) {
                sforce.one.navigateToURL(url);
            } else {
                top.location.href=url;
