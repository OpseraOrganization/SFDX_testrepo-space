    <currency>EUR</currency>
    <description>Check if key parameters from Opportunity Strategy tab are filled for TOP complex multiBG deals and LoA level A and A+ cases since the implementation of CRM feature</description>
    <filter>
        <booleanFilter>((1 AND 4) OR 2) AND 3 AND 5</booleanFilter>
        <criteriaItems>
            <column>FK_NAME</column>
            <columnToColumn>false</columnToColumn>
            <isUnlocked>true</isUnlocked>
            <operator>contains</operator>
            <value>Top Complex MultiBG</value>
        </criteriaItems>
        <criteriaItems>
            <column>Opportunity.Expected_LoA_Approval_Level__c</column>
            <columnToColumn>false</columnToColumn>
            <isUnlocked>true</isUnlocked>
            <operator>equals</operator>
